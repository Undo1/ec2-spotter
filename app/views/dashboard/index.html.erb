<h1>Dashboard#index</h1>
<p>Find me in app/views/dashboard/index.html.erb</p>

<table class="table">
<tr>
  <th>Availability Zone</th>
  <th>Instance Type</th>
  <th>Price</th>
</tr>
<% @spot_prices.each do |sp| %>
  <tr>
    <td><%= sp.availability_zone.api_name %></td>
    <td><%= sp.instance_type.api_name %></td>
    <td><%= number_to_currency sp.price, :precision => 4 %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @spot_prices %> 
